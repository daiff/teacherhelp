<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    <include layout="@layout/shjfx_top"/>

    <LinearLayout
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <LinearLayout
            style="@style/list_failure_tab">
            <TextView
                style="@style/list_failure_tab_left"
                android:text="90-100分:"/>
            <TextView
                android:id="@+id/fsh_9"
                style="@style/list_failure_tab_right"
                android:text=" "/>
            <TextView
                android:id="@+id/bl_9"
                style="@style/shjfxi_right_Textview"
                android:text=""/>
        </LinearLayout>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/background"/>

        <LinearLayout
            style="@style/list_failure_tab">
            <TextView
                style="@style/list_failure_tab_left"
                android:text="80-89.9分:"/>
            <TextView
                android:id="@+id/fsh_8"
                style="@style/list_failure_tab_right"
                android:text=""/>
            <TextView
                android:id="@+id/bl_8"
                style="@style/shjfxi_right_Textview"
                android:text=""/>
        </LinearLayout>
    </LinearLayout>
        <View
            style="@style/View_hen"/>

    <LinearLayout
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <LinearLayout
            style="@style/list_failure_tab">
            <TextView
                style="@style/list_failure_tab_left"
                android:text="70-79.9分:"/>
            <TextView
                android:id="@+id/fsh_7"
                style="@style/list_failure_tab_right"
                android:text=" "/>
            <TextView
                android:id="@+id/bl_7"
                style="@style/shjfxi_right_Textview"
                android:text=""/>
        </LinearLayout>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/background"/>

        <LinearLayout
            style="@style/list_failure_tab">
            <TextView
                style="@style/list_failure_tab_left"
                android:text="60-69.9分:"/>
            <TextView
                android:id="@+id/fsh_6"
                style="@style/list_failure_tab_right"
                android:text=""/>
            <TextView
                android:id="@+id/bl_6"
                style="@style/shjfxi_right_Textview"
                android:text=""/>
        </LinearLayout>
    </LinearLayout>
        <View
            style="@style/View_hen"/>

    <LinearLayout
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <LinearLayout
            style="@style/list_failure_tab">
            <TextView
                style="@style/list_failure_tab_left"
                android:text="0-59.9分:"/>
            <TextView
                android:id="@+id/fsh_0"
                style="@style/list_failure_tab_right"
                android:text=" "/>
            <TextView
                android:id="@+id/bl_0"
                style="@style/shjfxi_right_Textview"
                android:text=""/>
        </LinearLayout>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/background"/>

        <LinearLayout
            style="@style/list_failure_tab">
            <TextView
                style="@style/list_failure_tab_left"
                android:text="实考人数:"/>
            <TextView
                android:id="@+id/total_people"
                style="@style/list_failure_tab_right"
                android:text=" "/>
        </LinearLayout>
    </LinearLayout>
        <View
            style="@style/View_hen"/>

    <LinearLayout
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <LinearLayout
            style="@style/list_failure_tab">

            <TextView
                style="@style/list_failure_tab_left"
                android:text="最高分:"
                android:id="@+id/textView" />

            <TextView
                android:id="@+id/high_fen"
                style="@style/list_failure_tab_right"
                android:text=" "/>
        </LinearLayout>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/background"/>

        <LinearLayout
            style="@style/list_failure_tab">
            <TextView
                style="@style/list_failure_tab_left"
                android:text="最低分:"/>
            <TextView
                android:id="@+id/low_fen"
                style="@style/list_failure_tab_right"
                android:text=" "/>
        </LinearLayout>
    </LinearLayout>



        <LinearLayout
            android:paddingLeft="12dp"
            android:paddingRight="12dp"
            android:background="@color/white"
            android:layout_marginTop="12dp"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal"
            android:gravity="center_vertical">
            <ImageView
                android:layout_marginRight="10dp"
                android:layout_width="4dp"
                android:layout_height="match_parent"
                android:src="@drawable/item_left"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="综合分析"/>
        </LinearLayout>

    <TextView
        android:textSize="13dp"
        android:padding="12dp"
        android:layout_marginTop="0.5dp"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text=" "
        android:id="@+id/prop_analysis" />

    </LinearLayout>
    </ScrollView>
</LinearLayout>